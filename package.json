{
	"name": "elysia-supabase-template",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"start": "bun run src/index.ts",
		"check": "bunx tsc --noEmit",
		"format": "bunx biome format --write .",
		"lint": "bunx biome check --write .",
		"prisma:generate": "bunx prisma generate",
		"prisma:migrate": "bunx prisma migrate dev",
		"prisma:studio": "bunx prisma studio",
		"prisma:seed": "bunx prisma db seed",
		"email:dev": "bunx react-email dev --dir src/emails --port 3001",
		"supabase:keepalive": "bun run src/scripts/keepalive.ts"
	},
	"prisma": {
		"seed": "bun run prisma/seed.ts"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.4.1",
		"@elysiajs/cron": "^1.4.1",
		"@elysiajs/openapi": "^1.4.13",
		"@prisma/adapter-pg": "^7.2.0",
		"@prisma/client": "7.2.0",
		"@react-email/components": "^1.0.4",
		"@react-email/render": "^2.0.2",
		"elysia": "^1.4.22",
		"elysia-logger": "^0.5.2",
		"elysia-rate-limit": "^4.4.2",
		"@supabase/supabase-js": "^2.50.2",
		"pg": "^8.17.1",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"resend": "^6.7.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@react-email/preview-server": "5.2.5",
		"@types/hibp": "^7.3.0",
		"@types/node": "^25.0.9",
		"@types/pg": "^8.16.0",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"bun-types": "^1.3.6",
		"dotenv": "^17.2.3",
		"esbuild": "^0.27.2",
		"hibp": "^15.2.0",
		"prisma": "7.2.0",
		"typescript": "^5.9.3"
	}
}
